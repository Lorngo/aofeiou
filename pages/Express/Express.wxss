/* pages/Express/Express.wxss */

page {
  background-color: white;
}

.news {
  width: 695rpx;
  margin: 0rpx auto;
}

.news .li {
  width: 100%;
  height: 42rpx;
  display: flex;
  justify-content: space-between;
  margin-top: 39rpx;
  padding-bottom: 36rpx;
  border-bottom: 1rpx solid #EFEFEF;
  font-size: 30rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #333333;
}

.news .last {
  border: none;
}

.default,.but{
  width: 594rpx;
  height: 94rpx;
  border-radius: 15rpx;
  text-align: center;
  line-height: 94rpx;
  margin: 0rpx auto;
}

.default{
  margin-top: 76rpx;
  font-size: 36rpx;
font-family: PingFang SC, PingFang SC-Regular;
font-weight: 400;
color: #303030;
background-color: #EAEAEA;
}

.but{
  font-size: 36rpx;
font-family: PingFang SC, PingFang SC-Regular;
font-weight: 400;
color: #ffffff;
margin-top: 48rpx;
background-color: #383838;
}