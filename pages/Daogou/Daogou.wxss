/* pages/Daogou/Daogou.wxss */
page {
  background-color: white;
}

.head {
  width: 750rpx;
  height: 145rpx;
  display: flex;
  align-items: center;
  background-color: #F6F7F7;
  justify-content: space-between;
}

.head .left {
  margin-left: 27rpx;
  display: flex;
  width: 210rpx;
  justify-content: space-between;
  align-items: center;
 
}

.head .left .img {
  width: 83rpx;
  height: 83rpx;
  border-radius: 50%;
  overflow: hidden;
}

.head .left .img image {
  width: 83rpx;
  height: 83rpx;

}

.head .left .name {
  width: 100rpx;
  margin-left: 26rpx;
  font-size: 30rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
}

.head .right {
  width: 45rpx;
  height: 45rpx;
  margin-right: 28rpx;
}

.head .right image {
  width: 100%;
  height: 100%;
}

.list .box {
  width: 694rpx;
  margin-left: 28rpx;
  margin-top: 30rpx;
  padding-bottom: 36rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #f0f0f0;
  font-size: 30rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  color: #333333;
}

.list .box .right{
  flex: 1;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  text-align: right;
}

.list .box .change{
  width: 540rpx;
  text-align: right;
}

.list .box .left{
  width:120rpx;
}



